<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name=viewport content="width=device-width, initial-scale=1">
  <title>Random | Delete Video</title>
  <link rel="icon" href="/images/favicon.ico">
  <link rel="stylesheet" type="text/css" href="/Css/loading.css">
  <script>
    function hideLoading() {
      document.getElementById('loading').style.opacity = '0';
      document.getElementById('loading').style.zIndex = '-1';
    }
  </script>
  <!-- Style.css-->
  <link rel="stylesheet" href="fontawesome-all.min.css">

  <!-- Font fontawesome-->
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.13.0/css/all.css">
  <link href="https://fonts.googleapis.com/css?family=Montserrat|Ubuntu" rel="stylesheet">


  <!-- bootstrap and jQuary-->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="/Css/pills.css">
  <!-- JS, Popper.js, and jQuery -->
  <style>
    body {
      background-color: #c1a1d3;
    }

    .row {
      margin: 0 !important;
      padding: 0 !important;
    }

    .centered {
      color: #fff;
      margin-top: 10px;
      margin-bottom: 10px;
      text-align: center;
      font-family: 'Ubuntu';
      font-size: 32px;
      text-shadow: 2px 2px #31312d;
    }

    iframe {
      margin: 30px 0 0 30px;

    }

    .row {
      margin-left: 0px !important;
      margin-right: 0px !important;
    }

    .container {
      padding-left: 0px !important;
      padding-right: 0px !important;
    }

    @media (max-width:995px) {
      iframe {
        margin: 30px 0 0 9px;

      }
    }

    @media (max-width:500px) {
      iframe {
        margin: 30px 0px 0 0px;

      }

    }
  </style>
</head>

<body onload="hideLoading();">

  <div id="loading">
    <div class="spinner-box">
      <div class="pulse-container">
        <div class="pulse-bubble pulse-bubble-1"></div>
        <div class="pulse-bubble pulse-bubble-2"></div>
        <div class="pulse-bubble pulse-bubble-3"></div>
      </div>
    </div>
  </div>
  <div class="centered">Videos</div>

  <div class="row container">
    <div class="col-lg-5 col-md-4 ad"></div>
    <div class="col-lg-5 col-md-7 col-sm-12" id="accordionExample">

      <ul class="nav nav-pills mb-3 " id="pills-tab" role="tablist">

        <li class="nav-item" role="presentation">
          <a class="nav-link inactive active " id="pills-news-tab" data-toggle="pill" href="#pills-news" role="tab" aria-controls="pills-profile" aria-selected="false">Painting</a>
        </li>

        <li class="nav-item" role="presentation">
          <a class="nav-link inactive" id="pills-judiciary-tab" data-toggle="pill" href="#pills-judiciary" role="tab" aria-controls="pills-contact" aria-selected="false">Sketch</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link inactive" id="pills-last-tab" data-toggle="pill" href="#pills-last" role="tab" aria-controls="pills-contact" aria-selected="false">Portrait</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link inactive " id="pills-article-tab" data-toggle="pill" href="#pills-article" role="tab" aria-controls="pills-home" aria-selected="true">Tutorial</a>
        </li>
      </ul>



    </div>
    <div class="col-lg-2 col-md-1"></div>
  </div>
  <div class="tab-content" id="pills-tabContent">

    <div class="tab-pane active " id="pills-news" role="tabpanel" aria-labelledby="pills-news-tab">
      <div class="row ">
        <%  painting.forEach(function(video){ %>

        <div class="col-lg-4 col-md-6 " style="text-align: center;">

          <iframe src="https://www.youtube.com/embed/<%=video.url%>" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          <br>
          <form action="/delete/<%= video._id%>?_method=DELETE" method="POST">
            <button type="submit" class="btn btn-outline-danger">
              Delete </button>
          </form>


        </div>

        <% }) %>
      </div>




    </div>
    <div class="tab-pane" id="pills-judiciary" role="tabpanel" aria-labelledby="pills-judiciary-tab">

      <div class="row ">
        <%  sketch.forEach(function(video){ %>

        <div class="col-lg-4 col-md-6 " style="text-align: center;">

          <iframe src="https://www.youtube.com/embed/<%=video.url%>" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          <br>
          <form action="/delete/<%= video._id%>?_method=DELETE" method="POST">
            <button type="submit" class="btn btn-outline-danger">
              Delete </button>
          </form>


        </div>

        <% }) %>
      </div>

    </div>
    <div class="tab-pane" id="pills-last" role="tabpanel" aria-labelledby="pills-last-tab">
      <div class="row ">
        <%  portrait.forEach(function(video){ %>

        <div class="col-lg-4 col-md-6 " style="text-align: center;">

          <iframe src="https://www.youtube.com/embed/<%=video.url%>" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          <br>
          <form action="/delete/<%= video._id%>?_method=DELETE" method="POST">
            <button type="submit" class="btn btn-outline-danger">
              Delete </button>
          </form>


        </div>

        <% }) %>
      </div>
    </div>
    <div class="tab-pane" id="pills-article" role="tabpanel" aria-labelledby="pills-article-tab">
      <div class="row ">
        <%  tutorial.forEach(function(video){ %>

        <div class="col-lg-4 col-md-6 " style="text-align: center;">

          <iframe src="https://www.youtube.com/embed/<%=video.url%>" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          <br>
          <form action="/delete/<%= video._id%>?_method=DELETE" method="POST">
            <button type="submit" class="btn btn-outline-danger">
              Delete </button>
          </form>


        </div>

        <% }) %>
      </div>
    </div>
  </div>





</body>

<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="/bootstrap/js/bootstrap.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="/js/index.js"></script>

</html>
